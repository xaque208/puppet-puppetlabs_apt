{
  "name": "ploperations-puppetlabs_apt",
  "version": "0.0.6",
  "author": "Puppet Labs Operations",
  "summary": "Installs the Puppet Labs community apt repository",
  "license": "Apache 2.0",
  "source": "https://github.com/puppetlabs-operations/puppet-puppetlabs_apt",
  "project_page": "https://github.com/puppetlabs-operations/puppet-puppetlabs_apt",
  "issues_url": "https://github.com/puppetlabs-operations/puppet-puppetlabs_apt/issues",
  "dependencies": [
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.0.1"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ]
}
